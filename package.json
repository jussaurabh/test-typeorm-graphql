{
	"name": "test-typeorm-graphql",
	"version": "0.0.1",
	"description": "Awesome project developed with TypeORM.",
	"devDependencies": {
		"@types/jest": "^25.1.4",
		"@types/node": "^8.10.59",
		"jest": "^25.1.0",
		"nodemon": "^2.0.2",
		"ts-jest": "^25.2.1",
		"ts-node": "^3.3.0",
		"typescript": "^3.3.3333"
	},
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"graphql-import": "^1.0.1",
		"graphql-request": "^1.8.2",
		"graphql-yoga": "^1.18.3",
		"mongodb": "^3.0.8",
		"reflect-metadata": "^0.1.10",
		"typeorm": "0.2.24"
	},
	"scripts": {
		"server": "NODE_ENV=development nodemon --exec ts-node src/index.ts",
		"test-server": "NODE_ENV=test ts-node src/index.ts",
		"test": "NODE_ENV=test jest --detectOpenHandles"
	},
	"jest": {
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
		"moduleFileExtensions": [ "ts", "tsx", "js", "jsx", "json", "node" ]
	}
